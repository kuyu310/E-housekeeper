platform :ios, '9.0'
use_frameworks!
target 'E-housekeeper' do
pod 'SVProgressHUD'
# Alamofire 网络库 （4.1.0最低支持iOS8.0,4.0最低支持iOS9.0）
pod 'Alamofire', '~> 4.4'
# swift解析json库
pod 'SwiftyJSON'
#网络图片库
pod 'Kingfisher', '~> 3.0'
#字典和模型之间互相转换的超轻量级框架（3.0.13）
pod 'MJExtension'
# swift编写的各种加密算法，如：MD5，SHA1
pod 'CryptoSwift'
# swift RSA加密
pod 'SwiftyRSA'
#二维码
pod 'SwiftQRCode'
#多种类型弹出框
pod 'SCLAlertView'
# 苹果底层保存密码keychain工具
pod 'KeychainSwift', '~> 6.0'
# 日志输出管理
pod 'Log', '~> 1.0'
#Ramotion出品,给tabbar items各单元添加动画效果
pod 'RAMAnimatedTabBarController', "~> 2.0.13"
#卡片cell，可以折叠 (2.0.3)
pod 'FoldingCell'
#友盟分享
pod ‘UMengSocial’, ‘~> 5.0’
#userdefault封装库，可以很愉快的使用userdefaults
pod 'SwiftyUserDefaults'
#图示控件(3.0.1)
pod 'Charts'
#用户指导页(2.3.1)
pod 'Onboard'
#设备信息
pod 'DeviceKit', '~> 1.0'
#用代码轻松auto layout
pod 'SnapKit'
#基于CoreBluetooth的封装
pod 'BabyBluetooth','~> 0.7.0'
pod 'WeexSDK'
pod 'WXDevtool', '0.8.0'
pod 'SDWebImage', '3.7.5'
end
